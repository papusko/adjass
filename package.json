{
  "name": "Adjaas",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "node .scripts/generate-env.js ./public/env",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "node .scripts/post-build.js && node .scripts/generate-env.js --production ./build/env",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --coverage",
    "bdd": "cucumber-js --profile default",
    "sonar": "ts-node sonar-scanner.ts",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .tsx,.ts,.js,.jsx ./src/",
    "generate": "plop"
  },
  "engines": {
    "npm": ">=6",
    "node": ">=12"
  },
  "eslintConfig": {
    "plugins": [
      "sonarjs",
      "jsx-a11y",
      "prettier"
    ],
    "extends": [
      "react-app",
      "plugin:jsx-a11y/recommended",
      "plugin:sonarjs/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "curly": "error"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": 0,
          "@typescript-eslint/explicit-member-accessibility": 0
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "resetMocks": false
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 120,
    "endOfLine": "auto"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.15",
    "@mui/lab": "^5.0.0-alpha.109",
    "@mui/material": "^5.10.12",
    "@mui/styled-engine-sc": "^5.10.6",
    "@mui/styles": "^5.10.15",
    "axios": "^0.27.2",
    "easy-peasy": "5.0.4",
    "jest-fetch-mock": "^3.0.3",
    "jest-html-reporter": "^3.7.0",
    "msw": "^0.47.4",
    "react": "17.0.2",
    "react-bootstrap": "^2.6.0",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.2.3",
    "react-intl": "5.20.10",
    "react-router-dom": "5.3.0",
    "styled-components": "^5.3.6"
  },
  "resolutions": {
    "@types/react": "17.0.20"
  },
  "overrides": {
    "@types/react": "17.0.20"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@cucumber/cucumber": "^8.7.0",
    "@datapunt/matomo-tracker-react": "0.5.1",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.2.1",
    "@types/cucumber": "^7.0.0",
    "@types/history": "4.7.11",
    "@types/jest": "27.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "17.0.20",
    "@types/react-dom": "17.0.9",
    "@types/react-router-dom": "5.1.8",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "chalk": "4.1.1",
    "cross-env": "7.0.3",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-sonarjs": "0.10.0",
    "full-icu": "1.3.4",
    "husky": "4.3.8",
    "jsdom-global": "^3.0.2",
    "lint-staged": "11.1.2",
    "plop": "2.7.4",
    "prettier": "2.3.2",
    "react-scripts": "4.0.3",
    "sass": "1.39.0",
    "semver": "7.3.5",
    "typescript": "4.4.2"
  },
  "author": "fatoumata.diarra@socgen.com",
  "bugs": "",
  "description": "Adjustments As A Service",
  "keywords": [
    "adjustment",
    "ajustement",
    "module",
    "generique",
    "service"
  ],
  "repository": "",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}